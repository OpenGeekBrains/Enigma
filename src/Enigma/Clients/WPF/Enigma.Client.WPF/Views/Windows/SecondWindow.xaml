<Window x:Class="Enigma.Client.WPF.Views.Windows.SecondWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Enigma.Client.WPF.Views.Windows"
        mc:Ignorable="d"
        Width="400"
        Height="200"
        FontSize="28"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterScreen"
        DataContext="{Binding Source={StaticResource Locator}, Path=SecondViewModel}">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Пераданый параметр:" />
            <TextBlock Text="{Binding Parameter}" Width="auto" />
        </StackPanel>
        
    </Grid>
</Window>
